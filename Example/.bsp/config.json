{
	"name": "sourcekit-bazel-bsp",
	"version": "0.1.0",
	"bspVersion": "2.2.0",
	"languages": [
		"c",
		"cpp",
		"objective-c",
		"objective-cpp",
		"swift"
	],
	"argv": [
		".bsp/sourcekit-bazel-bsp",
		"serve",
		"--target",
		"//HelloWorld:HelloWorld",
		"--target",
		"//HelloWorld:HelloWorldTests",
		"--target",
		"//HelloWorld:HelloWorldWatchApp",
		"--target",
		"//HelloWorld:HelloWorldWatchTests",
		"--target",
		"//HelloWorld:HelloWorldMacApp",
		"--target",
		"//HelloWorld:HelloWorldMacTests",
		"--bazel-wrapper",
		"bazelisk",
		"--index-flag",
		"config=index_build",
		"--files-to-watch",
		"HelloWorld/**/*.swift,HelloWorld/**/*.m,HelloWorld/**/*.h"
	]
}